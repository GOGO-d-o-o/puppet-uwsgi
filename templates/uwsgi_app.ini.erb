#
# This file is managed by puppet class 'uwsgi::app'
#
# uwsgi app config for '<%= @name %>'
#
[uwsgi]
uid = <%= @uid %>
gid = <%= @gid %>
<%
if @config
  @config.sort.each do |key, value|
    if value.is_a?(Array)
      value.each do |subvalue| -%>
<%= key %> = <%= subvalue %>
<%-   end
    else -%>
<%= key %> = <%= value%>
<%
    end
  end
end
-%>

